<h2>User Edit Page</h2>
<div class="well">
  <h4>Email: {{controller.email}}</h4> <br />
  <h4>Password: {{view Ember.TextField valueBinding="controller.password" type="password" placeholder="User password"}}</h4> <br />
  <button {{action cancel}} class="btn"> Cancel </button>  <button {{action update context="controller.content"}} class="btn btn-primary"> Submit </button>
</div>
