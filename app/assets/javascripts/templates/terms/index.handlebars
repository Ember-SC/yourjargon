<div class="container-fluid">
  <div class="row-fluid" id="bottom-scrollable">
    <div class="span6">
      <b> {{hello}} </b>
      <ul class="terms-list">
        {{#each term in controller.content}}
        <li>{{#linkTo terms.show term.id}} {{term.name}} {{/linkTo}} </li>
        {{/each}}
      </ul>
    </div>
    <div class="span6">
      {{view YJ.AlphabetView }}
    </div>
  </div>
</div>
